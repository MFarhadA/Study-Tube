{
  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Brave",
      "url": "http://localhost/Study-Tube/login/index.html",  // Ganti dengan URL aplikasi Anda
      "webRoot": "${workspaceFolder}",
      "runtimeExecutable": "C:/Program Files/BraveSoftware/Brave-Browser/Application/brave.exe",  // Ganti dengan path Brave di sistem Anda (contoh untuk macOS)
      "runtimeArgs": [
        "--remote-debugging-port=9222"
      ]
    }
  ]
}
